{% extends "base.twig" %}
{% block title %}問題一覧{% endblock %}

{% block content %}
    <div class="row">
        <table>
            <tr>
                <th>id</th>
                <th>title</th>
                <th>content</th>
                <th>date</th>
            </tr>
            {% for question in questionList %}
            <tr>
                {% for questionItem in question %}
                    <td>
                    {{questionItem}}
                    </td>
                {% endfor %}
            </tr>
            {% endfor %}
        </table>
    </div>
{% endblock %}
